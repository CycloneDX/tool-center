    "cycloneDxVersion": [
      "CYCLONEDX_V1.6",
      "CYCLONEDX_V1.5",
      "CYCLONEDX_V1.4",
      "CYCLONEDX_V1.3",
      "CYCLONEDX_V1.2"
    ],
    "supportedLanguages": [
      ".NET"
    ]
