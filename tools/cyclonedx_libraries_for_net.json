    "availability": [
      "OPEN_SOURCE"
    ],
    "functions": [
      "AUTHOR",
      "ANALYSIS",
      "TRANSFORM"
    ],
    "analysis": [
      "LICENSE_REPORTING"
    ],
    "transform": [
